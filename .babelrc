{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        [
            "airbnb"
        ],
        [
            "flow"
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        "@babel/plugin-proposal-object-rest-spread",
        "add-module-exports",
        [
            "@babel/plugin-transform-runtime"
        ]
    ],
    "ignore": [
        "node_modules",
        "dist"
    ]
}
